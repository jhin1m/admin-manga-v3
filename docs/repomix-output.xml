        >
          <UInput
            v-model="state.password"
            type="password"
            placeholder=""
            icon="i-lucide-lock"
            size="lg"
            autocomplete="current-password"
          />
        </UFormField>

        <UButton
          type="submit"
          block
          size="lg"
          :loading="auth.isLoading.value"
